{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "MovieTicketBookingDB": ""
    },
    "Jwt": {
        "Key": "",
        "Issuer": "MovieBookingAPI",
        "Audience": "MovieBookingUsers",
        "ExpireMinutes": 60
    },
    "SeatLock": {
        "LockDurationMinutes": 5
    },

    "Jobs": {
        "SeatLockExpiryIntervalSeconds": 60,
        "PendingBookingCleanupIntervalMinutes": 5
    },

    "SignalR": {
        "UseRedis": false,
        "RedisConnection": ""
    }

}
